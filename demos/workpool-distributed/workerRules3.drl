package workpool
import workpool.*;
rule "AdaptUsageFactor"
	when 
		$workerBean: WorkpoolBean(usageFactor > 0.8)
	then
		$workerBean.addWorkerToNode("");
end
rule "AdaptQueueFull"
	when
		$workerBean: WorkpoolBean((estimedQueueSize > 1900) && jobsComputed > 100)
	then	
		$workerBean.addWorkerToNode("nodeB")
end 