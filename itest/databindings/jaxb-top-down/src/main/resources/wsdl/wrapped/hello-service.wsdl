            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="HelloServiceService">
        <wsdl:port name="HelloServicePort"
            binding="tns:HelloServiceBinding">
            <soap11:address location="http://localhost:8085/hs-ep" />
        </wsdl:port>
    </wsdl:service>
